pipeline {
  agent any 
    stages {
      stages('first') {
        steps {
            echo "this is first stage"
        }
    }
    stages('second') {
        steps {
          echo "this is second stage"
        }
    }
    stages('third') {
        steps {
           echo "this is third stage"
        }
    }
    stages('forth') {
        steps {
          echo "this is forth stage"
        }
    }
}
 
